# coffee/cafe break

function cafe {
while true
do
    dd if=/dev/urandom bs=1024 count=128 2>/dev/null | (hexdump -e '"%08_ax  " 8/1 "%02x " "  " 8/1 "%02x " "  " 8/1 "%02x " "  " 8/1 "%02x " "  " 8/1 "%02x " "  " 8/1 "%02x " "  |" 48/1 "%_p" "|\n"' | grep --color=always "ca fe")
    sleep 0.1
done
}
