# startup
hash cpu-governor &>/dev/null && cpu-governor performance

gnome-screensaver &
xset dpms 0 900 0
~/local/bin/urxvtd -q -o -f
xsetroot -name "`date \"+%A %B %d, %Y\"` -- $USER@`hostname`"
wmname LG3D
display -window root .dwm/background.jpg

# window manager
ssh-agent zsh -i -c ~/local/bin/dwm

# shutdown
hash cpu-governor &>/dev/null && cpu-governor ondemand
exec echo
